// SECTION pagination :: START

section#pagination.pagination
  .pagination__wrapper
    a.pagination__btn.pagination__btn-first(href="#" class="" title="" disabled)!= "&#60;&#60;"
    a.pagination__btn.pagination__btn-prev(href="#" class="" title="" disabled)!= "&#60;"

    .pagination__btn-wrapper
      a.pagination__btn.pagination__btn-num.is-active(href="#" class="" title="")!= "1"
      a.pagination__btn.pagination__btn-num(href="#" class="" title="")!= "2"
      a.pagination__btn.pagination__btn-num(href="#" class="" title="")!= "3"
      a.pagination__btn.pagination__btn-num(href="#" class="" title="")!= "4"
      a.pagination__btn.pagination__btn-num(href="#" class="" title="")!= "5"

      span.pagination__btn.pagination__btn-empty!= "..."

      a.pagination__btn.pagination__btn-num(href="#" class="" title="")!= "10"

    a.pagination__btn.pagination__btn-next(href="#" class="" title="")!= "&#62;"
    a.pagination__btn.pagination__btn-last(href="#" class="" title="")!= "&#62;&#62;"

// SECTION pagination :: END