---
title: Fortnite - History sub

pageClassName: p-history
---
extends shared/_layout

block content
  div.body(class!= pageClassName)

    include components/common/_mainBanner

    section.section
      .c-grid
        .section__wrapper
          .section__wrapper-left

            section#history

              include ./components/history/_history-rotation
              include ./components/history/_history-sub-stats
              include ./components/history/_history-latest

              include ./components/common/_breadcrumbs

              include ./components/beginners/_beginners-comments

          .section__wrapper-right

            include components/common/_latest
            include components/common/_top
            include components/common/_sectionBanner
            include components/common/_royal

    include components/common/_reply

    section.section
      .c-grid
        .section__wrapper
          .section__wrapper-left
            include components/common/_comments

          .section__wrapper-right
